<extension xmlns="http://ns.adobe.com/air/extension/18.0">
    <id>io.teak.megamaid.Megamaid</id>
    <versionNumber>0.0.1</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment/>
        </platform>

        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>libMegamaidAir.jar</nativeLibrary>
                <initializer>io.teak.megamaid.Extension</initializer>
                <finalizer>io.teak.megamaid.Extension</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
